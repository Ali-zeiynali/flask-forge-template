SECURITY_HEADERS_ENABLED=true
ADMIN_SEED_ENABLED=true
ENABLE_HSTS=false
FORCE_HTTPS=false

DOCS_URL=/docs/index.md
GITHUB_URL=https://github.com/example/flask-forge-template
CI_STATUS=Passing
